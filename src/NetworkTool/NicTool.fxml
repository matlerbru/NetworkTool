                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="10.0" minWidth="10.0" prefWidth="10.0" />
                            </columnConstraints>
                            <rowConstraints>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                            </rowConstraints>
                            <children>
                                <Label text="IP:" />
                                <Label text="Subnet mask:" GridPane.rowIndex="1" />
                                <Label text="Default gateway:" GridPane.rowIndex="2" />
                                <TextField fx:id="IP" alignment="CENTER" disable="true" onAction="#ipEvent" onInputMethodTextChanged="#ipEvent" onKeyPressed="#ipEvent" onKeyReleased="#ipEvent" onKeyTyped="#ipEvent" GridPane.columnIndex="1" GridPane.halignment="CENTER" />
                                <TextField fx:id="subnetMask" alignment="CENTER" disable="true" onAction="#subnetMaskEvent" onInputMethodTextChanged="#subnetMaskEvent" onKeyPressed="#subnetMaskEvent" onKeyReleased="#subnetMaskEvent" onKeyTyped="#subnetMaskEvent" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="1" />
                                <TextField fx:id="defaultGateway" alignment="CENTER" disable="true" onAction="#DefaultGatewayEvent" onInputMethodTextChanged="#DefaultGatewayEvent" onKeyPressed="#DefaultGatewayEvent" onKeyReleased="#DefaultGatewayEvent" onKeyTyped="#DefaultGatewayEvent" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="2" />
                                <Label text="MAC:" GridPane.columnIndex="3" />
                                <TextField fx:id="macAdress" alignment="CENTER" disable="true" editable="false" GridPane.columnIndex="4" GridPane.halignment="CENTER" />
                                <Label text="DHCP:" GridPane.columnIndex="3" GridPane.rowIndex="1" />
                                <CheckBox fx:id="dhcp" disable="true" mnemonicParsing="false" onAction="#dhcpEvent" GridPane.columnIndex="4" GridPane.halignment="CENTER" GridPane.rowIndex="1" />
                                <Label text="Name:" GridPane.columnIndex="3" GridPane.rowIndex="2" />
                                <TextField fx:id="name" disable="true" onAction="#nameEvent" onInputMethodTextChanged="#nameEvent" onKeyPressed="#nameEvent" onKeyReleased="#nameEvent" onKeyTyped="#nameEvent" GridPane.columnIndex="4" GridPane.halignment="CENTER" GridPane.rowIndex="2" />
