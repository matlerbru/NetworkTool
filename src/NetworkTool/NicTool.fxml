                            </children>
                        </GridPane>
                    </bottom>
                    <top>
                        <Pane prefHeight="10.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
                    </top>
                    <left>
                        <Pane prefHeight="200.0" prefWidth="10.0" BorderPane.alignment="CENTER" />
                    </left>
                    <right>
                        <Pane prefHeight="200.0" prefWidth="10.0" BorderPane.alignment="CENTER" />
                    </right>
                </BorderPane>
                <BorderPane>

                    <top>
                        <GridPane alignment="center" nodeOrientation="LEFT_TO_RIGHT">
                            <children>
                                <ComboBox fx:id="NIC" maxWidth="1.7976931348623157E308" minWidth="0.0" onAction="#updateNicSettings" promptText="Select" visibleRowCount="20" GridPane.columnIndex="1" GridPane.rowIndex="1" />
