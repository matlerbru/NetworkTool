<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import NetworkInterfaceTool.*?>
<?import javafx.scene.layout.*?>

<TabPane fx:id="tabPane" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="NetworkTool.TabSelector">
    <Tab fx:id="NicSettingsTab" closable="false" text="NIC settings">
        <fx:include fx:id="nicTool" source="../Resources/NicTool.fxml" />
    </Tab>
    <Tab fx:id="NetworkScannerTab" closable="false" text="Network scanner">
        <fx:include fx:id="networkScanner" source="../Resources/NetworkScanner.fxml" />
    </Tab>
</TabPane>